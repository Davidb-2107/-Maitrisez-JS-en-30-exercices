var a = 1;
var b = 5;
console.log("--------------------------Avant inversion------------------");
console.log("A: " + a)
console.log("B: " + b)

//INVERSION
var tmp = a;
var a = b;
var b = tmp;

console.log("--------------------------Apr√®s inversion------------------");
console.log("A: " + a)
console.log("B: " + b)
